{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "webapp/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "builds": [
    {
      "src": "webapp/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/webapp/dist/$1"
    }
  ]
}
